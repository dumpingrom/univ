
public class NOT extends Composant{
	NOT() {
		this.entrees = new Composant[1];
		this.entrees[0] = null;
		this.sortie = null;
		this.setEtat(0);
	}

	@Override
	void miseAJour() {
		// TODO Auto-generated method stub
		if(this.entrees[0].getEtat() == 1) {
			this.setEtat(0);
		}
		else {
			this.setEtat(1);
		}
		System.out.println(this + ": " + this.getEtat());
	}

}
